{
  "name": "marrange",
  "displayName": "Marrange",
  "description": "Enables sorting for all kind of stuff in TypeScript",
  "version": "0.0.1",
  "publisher": "DrMueller",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.arrangeFile"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.arrangeFile",
        "title": "Arrange File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "lodash": "^4.17.4",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.57",
    "lodash": "^4.17.4"
  }
}
